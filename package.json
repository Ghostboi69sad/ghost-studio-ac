{
  "name": "ghoststudioacademy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:creator": "next dev -p 3006",
    "dev:listing": "next dev -p 3005",
    "build": "yarn install && next build",
    "build:creator": "cd src/app/components/course-creator && next build",
    "build:listing": "cd src/app/components/course-listing && next build",
    "start": "next start",
    "start:creator": "next start -p 3006",
    "start:listing": "next start -p 3005",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" --config .prettierrc",
    "backup": "ts-node src/scripts/manage-backups.ts backup",
    "restore": "ts-node src/scripts/manage-backups.ts restore",
    "netlify-build": "NEXT_PUBLIC_FIREBASE_PRIVATE_KEY=\"$(echo $FIREBASE_PRIVATE_KEY | base64 -d)\" next build"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.682.0",
    "@aws-sdk/client-s3": "^3.688.0",
    "@aws-sdk/cloudfront-signer": "^3.686.0",
    "@aws-sdk/s3-request-presigner": "^3.686.0",
    "@netlify/functions": "^2.8.2",
    "@netlify/plugin-nextjs": "^5.8.1",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/paypal-js": "^8.1.2",
    "@paypal/react-paypal-js": "^8.7.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@stripe/stripe-js": "^2.4.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "firebase": "^11.0.1",
    "firebase-admin": "^11.11.0",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "next-themes": "^0.4.3",
    "nextjs-cors": "^2.2.0",
    "react": "^18.2.0",
    "react-accordion": "^0.3.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-label": "^0.0.6",
    "react-progress": "^0.0.12",
    "react-toastify": "^10.0.6",
    "stripe": "13.2.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/json-schema": "^7.0.15",
    "@types/json5": "^0.0.30",
    "@types/node": "^22.9.0",
    "@types/paypal-rest-sdk": "^1.7.9",
    "@types/paypal__checkout-server-sdk": "^1.0.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
