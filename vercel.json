{
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": ".next",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/course-creator/(.*)",
      "dest": "/components/course-creator/$1"
    },
    {
      "src": "/course-listing/(.*)",
      "dest": "/components/course-listing/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://$VERCEL_URL"
  },
  "github": {
    "silent": true
  }
} 